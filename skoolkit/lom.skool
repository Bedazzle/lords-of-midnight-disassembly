 $F721 XOR A         ; Reset these variables.
 $F722 LD ($FF41),A  ;
 $F725 LD ($FF40),A  ;
*$F728 LD A,(HL)     ; Get the bytes.
*$F729 CP $FF        ; Was it a TERMINATOR?
 $F72B RET Z         ; Finished if it was.
 $F72C CP $FE        ; Are we expecting a LITERAL?
 $F72E JR Z,$F768    ;
 $F730 CP $FC        ; UPPERCASE Token?
 $F732 JR Z,$F751    ;
 $F734 CP $FB        ; NEWLINE
 $F736 JR Z,$F758    ;
*$F738 LD ($FF36),A  ; Token to decode.
 $F73B PUSH HL       ;
 $F73C CALL $F690    ;
 $F73F POP HL        ;
*$F740 INC HL        ;
 $F741 LD A,(HL)     ;
 $F742 CP $FE        ; LITERAL?
 $F744 JR Z,$F768    ;
 $F746 CP $FD        ; CONNECT token to last word
 $F748 JR Z,$F77D    ;
 $F74A PUSH HL       ; Time to print the last words
 $F74B CALL $F6C0    ; Made up in buffer.
 $F74E POP HL        ;
 $F74F JR $F71A      ; Loop all over again.
