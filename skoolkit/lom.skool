; Lookup a token in the token dictionary
;
; Tokens are stored using 5-bit bytes compressed together; the format is [length][length*char].
; All tokens have to be decoded until the required token is found.
@label=GetRequiredByte
c$FF80 PUSH HL       ;
 $FF81 LD D,H        ; HL=required byte
 $FF82 LD E,L        ;
 $FF83 ADD HL,HL     ; Multiply by 5
 $FF84 ADD HL,HL     ;
 $FF85 ADD HL,DE     ;
 $FF86 LD A,L        ;
 $FF87 AND $07       ; This is the same as
 $FF89 INC A         ; doing A=(HL MOD 8)+1
 $FF8A LD B,A        ;
 $FF8B SRL H         ; HL=HL/8
 $FF8D RR L          ; HL=Actual byte position in
 $FF8F SRL H         ; the data with B= to no of
 $FF91 RR L          ; rotations to get the required
 $FF93 SRL H         ; five bits.
 $FF95 RR L          ;
 $FF97 LD DE,($FF50) ;
 $FF9B ADD HL,DE     ;
 $FF9C LD E,(HL)     ;
 $FF9D INC HL        ;
 $FF9E LD D,(HL)     ;
 $FF9F JR $FFA5      ;
@label=Rotate_Once
*$FFA1 RR D          ; Rotate one bit
 $FFA3 RR E          ;
@label=Rotate_B_Times
*$FFA5 DJNZ $FFA1    ; Looop until finished
 $FFA7 LD A,E        ; A=Byte required
 $FFA8 AND $1F       ; Only need bottom five bits.
 $FFAA POP HL        ;
 $FFAB RET           ;

; Reads an ASCII character from the token dictionary
@label=GetASCIIchar
c$FFAC PUSH DE       ;
 $FFAD CALL $FF80    ; Get the byte from the data
 $FFB0 POP DE        ;
 $FFB1 OR $60        ; add on 'a'-1
 $FFB3 LD (DE),A     ; Store in buffer
 $FFB4 INC HL        ; Increase required byte
 $FFB5 INC DE        ; Increase position in buffer
 $FFB6 LD B,C        ;
 $FFB7 RET           ;

@label=DefineViewPoint
c$FFB8 POP HL        ;
 $FFB9 LD DE,$FF3A   ;
 $FFBC LD BC,$0004   ;
 $FFBF LDIR          ;
 $FFC1 LD A,(HL)     ;
 $FFC2 LD ($FF2D),A  ;
 $FFC5 INC HL        ;
 $FFC6 LD A,(HL)     ;
 $FFC7 LD ($FF2F),A  ;
 $FFCA XOR A         ;
 $FFCB LD ($FF42),A  ;
 $FFCE INC HL        ;
 $FFCF PUSH HL       ;
 $FFD0 RET           ;

